   "source": [
    "print(\"我来帮你检查N以前的所有质数！\")\n",
    "print(\"告诉我一个范围N!\")\n",
    "N=int(input())\n",
    "for x in range(2,N):\n",
    "    for n in range(2,x):\n",
    "        if x%n==0:\n",
    "            print(x,\"=\",n,\"*\",x//n,\"不是质数\")\n",
    "            break\n",
    "    else:\n",
    "            print(x,\"是质数\")\n"
   ]
