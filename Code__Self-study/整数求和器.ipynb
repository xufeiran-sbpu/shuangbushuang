   "source": [
    "print(\"我来帮你算从1到n的所有整数的和\")\n",
    "print(\"告诉我你想计算的n\")\n",
    "n=int(input())\n",
    "counter=1\n",
    "sum=0\n",
    "while counter<=n:\n",
    "    sum=sum+counter\n",
    "    counter+=1\n",
    "print(\"从1到%d的和为: %d\" %(n,sum))"
   ]
  }
